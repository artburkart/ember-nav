{{#with model}}
<h1 class="text-center row">{{title}}</h1>
<div class="row">
  <div class="list-group col-xs-12 col-md-6 col-md-offset-3">
    {{#if environments}}
    {{#each environments}}
      {{#link-to 'environment' this tagName="div" class="list-group-item clickable"}}
        {{#link-to 'environment' this}}{{title}}{{/link-to}}
      {{/link-to}}
    {{/each}}
    {{else}}
      <h1 class="text-center">No Environments Here</h1>
    {{/if}}
  </div>
</div>
{{/with}}